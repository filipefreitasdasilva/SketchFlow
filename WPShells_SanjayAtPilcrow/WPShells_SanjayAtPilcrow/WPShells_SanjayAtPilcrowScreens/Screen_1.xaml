		<pc:SketchRectangleUC HorizontalAlignment="Left" Height="60" Margin="69,333,0,0" Style="{DynamicResource Rectangle-Sketch}" VerticalAlignment="Top" Width="60"/>
		<pc:SketchRectangleUC HorizontalAlignment="Left" Height="60" Margin="147,333,0,0" Style="{DynamicResource Rectangle-Sketch}" VerticalAlignment="Top" Width="60"/>
